    "@types/jsonwebtoken": "^9.0.6",
    "@typescript/native-preview": "^7.0.0-dev.20260210.1",
    "fast-glob": "^3.3.3",
    "firebase-functions-test": "^3.4.1",
    "ts-to-zod": "^5.1.0",
    "tsc-alias": "^1.8.16",
    "type-fest": "^5.4.4",
    "vite": "npm:rolldown-vite@^7.3.1",
